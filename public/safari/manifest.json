{
  "manifest_version": 3,
  "name": "Brume Wallet",
  "description": "The first private Ethereum wallet with built-in Tor",
  "version": "0.1",
  "action": {
    "default_title": "Brume Wallet",
    "default_popup": "index.html"
  },
  "icons": {
    "512": "favicon.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval';"
  }
}
