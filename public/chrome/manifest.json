{
  "manifest_version": 3,
  "name": "Lunar Wallet",
  "description": "The private wallet",
  "version": "0.1",
  "action": {
    "default_title": "Lunar Wallet",
    "default_popup": "index.html"
  },
  "host_permissions": [
    "https://meek.bamsoftware.com/*"
  ],
  "content_security_policy": {
    "extension_pages": "default-src 'self'; script-src 'self' 'wasm-unsafe-eval'; connect-src https://meek.bamsoftware.com/;"
  }
}